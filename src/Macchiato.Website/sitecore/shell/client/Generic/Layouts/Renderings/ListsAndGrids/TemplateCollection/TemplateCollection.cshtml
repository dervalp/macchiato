@using Sitecore.Data.Items
@using Sitecore.Mvc
@using Sitecore.Web.UI
@model Sitecore.Mvc.Presentation.RenderingModel
@{
    var control = this.InitializeControl("TemplateCollection");
    var componentType = control.GetString("ComponentType");
    var componentScript = control.GetString("ComponentScript");
    var componentTemplate = control.GetString("ComponentTemplate");
    var TagName = control.GetString("TagName");    
}
<script type="text/sc-component" @control.HtmlAttributes data-sc-properties='{ "childTemplate": "@componentTemplate", "componentType": "@componentType", "componentScript": "@componentScript", "TagName": "@TagName" }' data-sc-presenter="scKoPresenter">
</script> 