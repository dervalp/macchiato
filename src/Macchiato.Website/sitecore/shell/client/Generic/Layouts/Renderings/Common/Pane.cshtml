@using Sitecore.Data.Items
@using Sitecore.Mvc
@using Sitecore.Web.UI
@model Sitecore.Mvc.Presentation.RenderingModel
@{
    var control = this.InitializeControl("pane");
    var controlId = control.ControlId;
}
<div @control.HtmlAttributes data-sc-presenter>
    <header>@control.GetString("Title")</header>
    <div class="pane-main">
        @this.Html.Sitecore().Placeholder(controlId + ".Content")
    </div>
</div>